<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/activity_main">

    <ImageView
        android:id="@+id/vegafon"
        android:layout_width="wrap_content"
        android:layout_height="270dp"
        android:layout_marginTop="180dp"
        android:contentDescription="@string/cont_str_main"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:srcCompat="@drawable/vegaf" />

    <View
        android:id="@+id/viewGradient"
        android:layout_width="750dp"
        android:layout_height="342dp"
        android:background="#131313"
        app:layout_constraintEnd_toEndOf="@+id/vegafon"
        app:layout_constraintStart_toStartOf="@+id/vegafon"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/breathMode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="40dp"
        android:text="@string/Breath"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vegafon" />

    <Button
        android:id="@+id/sequence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/sequence"
        app:layout_constraintEnd_toEndOf="@+id/breathMode"
        app:layout_constraintStart_toStartOf="@+id/breathMode"
        app:layout_constraintTop_toBottomOf="@+id/breathMode" />

    <com.example.v2vega109.ColorPicker
        android:id="@+id/colorPicker"
        android:layout_width="@dimen/pickerLength"
        android:layout_height="@dimen/pickerLength"
        android:layout_marginTop="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vegafon" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btnArray"
        android:layout_width="544dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colorPicker">

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb14"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb13"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"

            app:layout_constraintEnd_toStartOf="@+id/arrb14"
            app:layout_constraintTop_toTopOf="@+id/arrb14" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb12"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toStartOf="@+id/arrb13"
            app:layout_constraintTop_toTopOf="@+id/arrb13" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb11"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toStartOf="@+id/arrb12"
            app:layout_constraintTop_toTopOf="@+id/arrb12" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb15"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb14"
            app:layout_constraintTop_toTopOf="@+id/arrb14" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb16"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb15"
            app:layout_constraintTop_toTopOf="@+id/arrb15" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb17"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb16"
            app:layout_constraintTop_toTopOf="@+id/arrb16" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb24"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginTop="@dimen/lengthVertical"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/arrb14" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb23"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toStartOf="@+id/arrb24"
            app:layout_constraintTop_toTopOf="@+id/arrb24" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb22"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toStartOf="@+id/arrb23"
            app:layout_constraintTop_toTopOf="@+id/arrb23" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb21"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginEnd="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintEnd_toStartOf="@+id/arrb22"
            app:layout_constraintTop_toTopOf="@+id/arrb22" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb25"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb24"
            app:layout_constraintTop_toTopOf="@+id/arrb24" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb26"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb25"
            app:layout_constraintTop_toTopOf="@+id/arrb25" />

        <com.example.v2vega109.CircleButton
            android:id="@+id/arrb27"
            android:layout_width="@dimen/circleBtn"
            android:layout_height="@dimen/circleBtn"
            android:layout_marginStart="@dimen/lengthHorizontal"
            android:background="@drawable/circled_button"
            app:layout_constraintStart_toEndOf="@+id/arrb26"
            app:layout_constraintTop_toTopOf="@+id/arrb26" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view"
        android:layout_width="607dp"
        android:layout_height="0dp"
        android:background="#000000"
        android:elevation="-1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewGradient" />


</androidx.constraintlayout.widget.ConstraintLayout>